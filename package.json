{
  "name": "@swapi/source",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "start": "nx serve swapi",
    "lint": "nx run-many --target=lint --all=true",
    "prettier": "nx format:write",
    "format": "npm run prettier && npm run lint",
    "e2e": "nx run swapi-e2e:e2e",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@material-tailwind/react": "^2.1.4",
    "@swc/helpers": "~0.5.2",
    "@tanstack/react-query": "^4.35.3",
    "@tanstack/react-query-devtools": "^4.35.3",
    "axios": "^1.5.1",
    "classnames": "^2.3.2",
    "idb-keyval": "^6.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-loader-spinner": "^5.4.5",
    "react-particles": "^2.12.2",
    "react-router-dom": "^6.17.0",
    "semantic-release": "^22.0.5",
    "tslib": "^2.3.0",
    "tsparticles-preset-stars": "^2.12.0",
    "unplugin-fonts": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/eslintrc": "^2.1.1",
    "@eslint/js": "~8.46.0",
    "@nx/cypress": "17.0.1",
    "@nx/devkit": "17.0.1",
    "@nx/eslint": "17.0.1",
    "@nx/eslint-plugin": "17.0.1",
    "@nx/jest": "17.0.1",
    "@nx/js": "17.0.1",
    "@nx/playwright": "17.0.1",
    "@nx/react": "17.0.1",
    "@nx/vite": "17.0.1",
    "@nx/workspace": "17.0.1",
    "@playwright/test": "^1.36.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc-node/register": "~1.6.7",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.85",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.2",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.9",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-react": "~4.0.0",
    "@vitest/coverage-c8": "~0.32.0",
    "@vitest/ui": "~0.32.0",
    "autoprefixer": "10.4.13",
    "eslint": "~8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-playwright": "^0.15.3",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": ">=6",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jsdom": "~22.1.0",
    "lint-staged": ">=10",
    "nx": "17.0.1",
    "postcss": "8.4.21",
    "prettier": "^2.6.2",
    "tailwindcss": "3.2.7",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.1.3",
    "vite": "~4.3.9",
    "vitest": "~0.32.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "nx format:write",
      "nx run-many --target=lint --all=true"
    ],
    "*.{json,css,scss}": "nx format:write"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
